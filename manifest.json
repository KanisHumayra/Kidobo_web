    }
  ],
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "1280x720",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "KIDOBO Home Screen"
    }
  ],
  "shortcuts": [
    {
      "name": "Start Reading",
      "short_name": "Read",
      "description": "Start reading a new story",
      "url": "/?page=library",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Play Games",
      "short_name": "Games",
      "description": "Play educational games",
      "url": "/?page=games",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "scope_extensions": [],
  "capture_links": "existing-client-navigate",
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/plain": [".txt"],
        "application/pdf": [".pdf"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+kidobo",
      "url": "/?story=%s"
    }
  ],
  "share_target": {
    "action": "/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "permissions": [
    "notifications"
  ],
  "background": {
    "service_worker": "/sw.js",
    "type": "module"
  },
  "display_override": [
    "standalone",
    "minimal-ui",
    "window-controls-overlay"
  ],
  "id": "kidobo-reading-app",
  "scope": "/",
  "start_url": "/",
